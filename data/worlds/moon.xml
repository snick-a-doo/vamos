<?xml version="1.0"?>

<world name="Moon">
  <!-- Acceleration due to gravity -->
  <gravity>1.6</gravity>

  <atmosphere>
    <!-- Air density -->
	<density>0.0</density>
	<!-- Wind velocity -->
    <velocity>[ 0.0, 0.0, 0.0 ]</velocity>
  </atmosphere>

  <lighting>
    <!-- Direction to the light source -->
    <source-position>[ 1.0, 1.0, 1.0 ]</source-position>
	<!-- RGB for ambient light -->
	<ambient>[ 0.0, 0.0, 0.0 ]</ambient>
  </lighting>
</world>